<div class="picture-frame-outer">
  <div class="picture-frame-inner">
    <%= image_tag("LIRR2.jpg", size: '210') %>
  </div>
</div>

<div class="content">Listing rsvps</h1>
 
<table>
  <tr>
    <th>Name</th>
    <th>Yes</th>
    <th>Guest</th>
    <th>Weekend</th>
    <th>Email</th>
  </tr>
 
  <% @rsvps.each do |rsvp| %>
    <tr>
      <td><%= rsvp.name %></td>
      <td><%= rsvp.check %></td>
      <td><%= rsvp.guest %></td>
      <td><%= rsvp.weekend %></td>
      <td><%= rsvp.email %></td>
    </tr>
  <% end %>
</table>

</div>